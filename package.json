{
  "name": "jscs-fixer",
  "main": "./lib/jscsfixer",
  "version": "0.5.1",
  "description": "Atom package to run jscs --fix on your scripts",
  "files": [
    "keymaps/jscsfixer.cson",
    "menus/jscsfixer.cson",
    "lib/jscsfixer.js",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "package.json"
  ],
  "activationCommands": {
    "atom-workspace": "jscs-fixer:fix"
  },
  "scripts": {
    "codestyle": "jscs -p google lib/*.js"
  },
  "pre-commit": [
    "codestyle"
  ],
  "repository": "https://github.com/foliveira/jscs-fixer",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "jscs": "^1.12.0"
  },
  "devDependencies": {
    "pre-commit": "^1.0.6"
  }
}
